  "description": "Camo-inspired ODM for NeDB, built specifically for BigfootDS' needs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/SuperCamo.js"
    },
    "./NedbDocument": "./src/structures/documents/NedbDocument.js",
    "./NedbEmbeddedDocument": "./src/structures/documents/NedbEmbeddedDocument.js",
    "./NedbClient": "./src/structures/database/NedbClient.js",
    "./NedbBaseDocument": "./src/structures/documents/NedbBaseDocument.js"
  },
  "scripts": {
