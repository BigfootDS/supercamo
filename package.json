  "author": {
    "name": "Alex Stormwood",
    "email": "alex@bigfootds.com",
    "url": "https://github.com/AlexStormwood"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BigfootDS/supercamo/issues"
  },
  "homepage": "https://github.com/BigfootDS/supercamo#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@seald-io/nedb": "^4.0.4"
  },
  "devDependencies": {
    "clean-jsdoc-theme": "^4.3.0",
